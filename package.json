{
  "name": "swivel-js",
  "version": "1.0.0",
  "description": "Javascript library for working with the Swivel Finance Protocol",
  "main": "",
  "author": "Swivel Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Swivel-Finance/swivel-js/issues"
  },
  "homepage": "https://github.com/Swivel-Finance/swivel-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Swivel-Finance/swivel-js.git"
  },
  "keywords": [
    "Swivel",
    "Finance",
    "Javascript",
    "Typescript",
    "Badass"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --quiet --fix",
    "test": "mocha -r ts-node/register 'src/**/*.test.ts'",
    "build:clean": "rm -rf ./build",
    "build:dry": "tsc -noEmit -p ./tsbuild.json",
    "build": "npm run build:clean && tsc -p ./tsbuild.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.0.12",
    "@ethersproject/contracts": "^5.0.10",
    "@ethersproject/providers": "^5.0.20",
    "@ethersproject/wallet": "^5.0.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  }
}
